  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bytes": "3.1.1",
    "@types/chai": "4.3.4",
    "@types/express": "4.17.17",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.18",
    "@types/readable-stream": "2.3.15",
    "@types/supertest": "2.0.12",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "esbuild": "0.16.17",
    "eslint": "8.33.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.1.0",
    "mocha": "10.2.0",
    "rimraf": "4.4.0",
    "supertest": "6.3.3",
    "typescript": "5.0.4"
