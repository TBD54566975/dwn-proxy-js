  "dependencies": {
    "@tbd54566975/dwn-sdk-js": "0.0.33",
    "bytes": "3.1.2",
    "node-fetch": "3.3.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "multiformats": "11.0.2",
    "prom-client": "14.2.0",
    "readable-stream": "4.3.0",
    "response-time": "2.3.2",
    "uuid": "9.0.0",
    "ws": "8.12.0"
  }
